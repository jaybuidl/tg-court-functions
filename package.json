{
  "name": "tg-court-functions",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "netlify dev --no-open",
    "dev:invoke-update": "netlify dev:exec -- netlify functions:invoke update --port 8888",
    "dev:notify": "NODE_NO_WARNINGS=1 NODE_OPTIONS=--experimental-fetch yarn ts-node functions/util/notify.ts",
    "dev:draw-v2": "NODE_NO_WARNINGS=1 NODE_OPTIONS=--experimental-fetch yarn ts-node functions/util/v2/draw.ts",
    "bot-init": "ts-node ./config/botInit.ts",
    "generate": "yarn generate:gql && yarn generate:supabase",
    "generate:gql": "graphql-codegen --config codegen.yml",
    "generate:supabase": "./scripts/generateSupabaseTypes.sh",
    "postinstall": "yarn generate",
    "codegen": "cd subgraph-court-v1 && graph codegen",
    "build": "cd subgraph-court-v1 && graph build",
    "deploy-xdai": "cd subgraph-court-v1 && mustache config/xdai.json subgraph.template.yaml > subgraph.yaml && mustache config/xdai.json src/kleros-liquid.template > src/kleros-liquid.ts && yarn generate && graph deploy --product hosted-service shotaronowhere/kleros-v1-notifications-gnosis",
    "deploy-mainnet": "cd subgraph-court-v1 && mustache config/mainnet.json subgraph.template.yaml > subgraph.yaml && mustache config/mainnet.json src/kleros-liquid.template > src/kleros-liquid.ts  && yarn generate && graph deploy --product hosted-service shotaronowhere/kleros-v1-notifications"
  },
  "dependencies": {
    "@grammyjs/auto-retry": "^1.1.1",
    "@graphprotocol/graph-cli": "^0.58.0",
    "@graphprotocol/graph-ts": "0.31.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-graphql-request": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@logtail/node": "^0.4.12",
    "@netlify/functions": "^1.6.0",
    "@supabase/supabase-js": "^2.32.0",
    "@wagmi/core": "^1.3.10",
    "amqplib": "^0.10.3",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.7.0",
    "grammy": "^1.18.3",
    "graphql": "^16.8.0",
    "graphql-request": "^6.1.0",
    "http-status-codes": "^2.2.0",
    "mustache": "^4.2.0",
    "node-cache": "^5.1.2",
    "node-telegram-bot-api": "^0.61.0",
    "p-queue": "^6.6.2",
    "supabase": "^1.88.0",
    "viem": "^1.6.7"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.2",
    "@types/node-telegram-bot-api": "^0.61.7",
    "matchstick-as": "0.5.0",
    "netlify-cli": "^16.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "packageManager": "yarn@1.22.19",
  "volta": {
    "node": "16.20.2",
    "yarn": "1.22.19"
  }
}
